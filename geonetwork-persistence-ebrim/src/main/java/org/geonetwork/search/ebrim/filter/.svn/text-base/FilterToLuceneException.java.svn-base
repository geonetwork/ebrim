package org.geonetwork.search.ebrim.filter;

/**
 * Exception launched by the filter to encode an OCG Filter to a Lucene Query
 * 
 * @author Jose
 * 
 */
public class FilterToLuceneException extends Exception {

	private static final long serialVersionUID = 7382278247308397875L;

	public FilterToLuceneException(String msg) {
		super(msg);
	}

	public FilterToLuceneException(String msg, Throwable exp) {
		super(msg, exp);
	}
}
