package org.geonetwork.search.ebrim;

/**
 * Base exception for Search Engine
 * 
 * @author Jose
 * 
 */
public class SearchEngineException extends Exception {

	private static final long serialVersionUID = 3910154711381149213L;

	public SearchEngineException(String message) {
		super(message);
	}

	public SearchEngineException(Throwable ex) {
		super(ex);
	}

	public SearchEngineException(String message, Throwable ex) {
		super(message, ex);
	}

}
