<Filter xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ogc="http://www.opengis.net/ogc"
	xmlns="http://www.opengis.net/ogc"
	xsi:schemaLocation="http://www.opengis.net/ogc filter.xsd">
	<Or>
		<And>
			<PropertyIsEqualTo>
				<PropertyName>title</PropertyName>
				<Literal>africa</Literal>
			</PropertyIsEqualTo>
			<PropertyIsLessThan>
				<PropertyName>identifier</PropertyName>
				<Literal>50</Literal>
			</PropertyIsLessThan>
		</And>
		<PropertyIsEqualTo>
			<PropertyName>format</PropertyName>
			<Literal>xml</Literal>
		</PropertyIsEqualTo>
	</Or>
</Filter>