<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://geonetwork.org/gaap/domain/group" xmlns:tns="http://geonetwork.org/gaap/domain/user" targetNamespace="http://geonetwork.org/gaap/domain/user" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://geonetwork.org/gaap/domain/group" schemaLocation="group.xsd"/>
	<xs:complexType name="user">
