<?xml version="1.0"?>
<project>
    <parent>
        <artifactId>geonetwork-gaap</artifactId>
        <groupId>org.geonetwork</groupId>
        <version>3.0.0</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.geonetwork</groupId>
    <artifactId>geonetwork-gaap-web</artifactId>
    <packaging>war</packaging>
    <name>geonetwork-gaap-web</name>
    <version>3.0.0</version>
    <url>http://maven.apache.org</url>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.4</version>
        </dependency>
        <dependency>
            <groupId>org.geonetwork</groupId>
            <artifactId>geonetwork-gaap-services</artifactId>
            <version>3.0.0</version>
        </dependency>
        <dependency>
            <groupId>org.geonetwork</groupId>
            <artifactId>geonetwork-gaap-webservices-def</artifactId>
            <type>aar</type>
            <version>3.0.0</version>
        </dependency>
        <dependency>
            <groupId>org.apache.axis2</groupId>
            <artifactId>axis2-spring</artifactId>
            <version>1.4.1</version>
        </dependency>
        <dependency>
            <groupId>org.apache.axis2</groupId>
            <artifactId>axis2-jibx</artifactId>
            <version>1.4.1</version>
        </dependency>
        <dependency>
            <groupId>org.jibx</groupId>
            <artifactId>jibx-run</artifactId>
            <version>1.1.6a</version>
        </dependency>
        <dependency>
            <groupId>commons-lang</groupId>
            <artifactId>commons-lang</artifactId>
            <version>2.4</version>
        </dependency>
    </dependencies>
</project>