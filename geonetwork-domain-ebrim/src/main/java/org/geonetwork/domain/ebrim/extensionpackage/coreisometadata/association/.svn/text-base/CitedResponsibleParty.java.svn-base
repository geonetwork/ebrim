package org.geonetwork.domain.ebrim.extensionpackage.coreisometadata.association;

import org.geonetwork.domain.ebrim.informationmodel.association.Association;
import org.geonetwork.domain.ebrim.informationmodel.core.datatype.URI;

/**
 * The association type is classified with the CitedResponsibleParty classification scheme that specifies 
 * the role of the organization (corresponds to MD_RoleCode).
 * 
 * Name and position information for an individual or organization that is responsible for the resource. 
 * The association Type has a set of subtypes operating to the same object types: PointOfCOntact, Author, 
 * Originator, Publisher.
 * 
 * Source object type:
 * urn:x-ogc:specification:csw-ebrim-cim:ObjectType:CitedItem
 * 
 * Target object type:
 * urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:Organization
 * 
 * @author heikki doeleman
 *
 */
public class CitedResponsibleParty extends Association {

	public CitedResponsibleParty() {
		super();
		this.associationType = new URI("urn:x-ogc:specification:csw-ebrim-cim:AssociationType:CitedResponsibleParty");
	}
}
