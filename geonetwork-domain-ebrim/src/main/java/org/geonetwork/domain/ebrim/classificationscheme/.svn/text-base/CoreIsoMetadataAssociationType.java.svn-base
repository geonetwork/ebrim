package org.geonetwork.domain.ebrim.classificationscheme;

import org.geonetwork.domain.ebrim.informationmodel.classification.ClassificationScheme;
import org.geonetwork.domain.ebrim.informationmodel.core.datatype.URI;
import org.geonetwork.domain.ebrim.informationmodel.core.datatype.URN;

/**
 * This is the ClassificationScheme for association types defined in OGC 07-038 CIM.
 * 
 * @author heikki doeleman
 *
 */
public class CoreIsoMetadataAssociationType extends ClassificationScheme {

	public CoreIsoMetadataAssociationType() {
		super();
		this.lid = new URN("urn:ogc:def:ebRIM-ClassificationScheme:AssociationType");
		this.id = new URN("urn:ogc:def:ebRIM-ClassificationScheme:AssociationType");
		this.setIsInternal(true);
		this.setNodeType(new URI("urn:oasis:names:tc:ebxml-regrep:NodeType:UniqueCode"));
		this.objectType = new URI("urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationScheme");

	// TODO name, description
		
		
	}
}
