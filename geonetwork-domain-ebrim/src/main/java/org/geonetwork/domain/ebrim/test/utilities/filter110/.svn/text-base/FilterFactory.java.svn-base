	public synchronized static Filter createDWithin() {
		Filter filter = new Filter();
		
		DWithin dwithin = new DWithin();
		
		Distance distance = new Distance();
		distance.setUnits("kilohertz");
		dwithin.setDistance(distance);
		
		Solid solid = new Solid();
		NCNameList ncnameList = new NCNameList();
		List<NCName> ncnames = new ArrayList<NCName>();
		NCName NCName = new NCName();
		NCName.setValue("NCName");
		ncnames.add(NCName);
		ncnameList.setValues(ncnames);
		solid.setAxisLabels(ncnameList);
		
		solid.setGid("gidstring");
		solid.setSrsDimension(2);
		solid.setSrsName("http://www.playboy.com.br");
		solid.setUomLabels(ncnameList);
		solid.setId("ID_1");
		
		
		dwithin.setGeometry(solid);
		
		PropertyName propertyName = new PropertyName();
		propertyName.setValue("dwithin-text");
		dwithin.setPropertyName(propertyName);
		
		filter.setOperator(dwithin);
		
		return filter;
	}
	
