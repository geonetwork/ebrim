	public synchronized static Filter createPropertyIsLessThan() {
		Filter filter = new Filter();
		
		PropertyIsLessThan propertyIsLessThan = new PropertyIsLessThan();
		
		Add add1 = new Add();
		PropertyName propertyName1 = new PropertyName();
		propertyName1.setValue("text-one");
		add1.setExpression1(propertyName1);
		PropertyName propertyName2 = new PropertyName();
		propertyName2.setValue("text-two");
		add1.setExpression2(propertyName2);
		
		Add add2 = new Add();
		PropertyName propertyName3 = new PropertyName();
		propertyName3.setValue("text-three");
		add2.setExpression1(propertyName3);
		PropertyName propertyName4 = new PropertyName();
		propertyName4.setValue("text-four");
		add2.setExpression2(propertyName4);
		
		propertyIsLessThan.setExpression1(add1);
		propertyIsLessThan.setExpression2(add2);
		propertyIsLessThan.setMatchCase(true);
		
		filter.setOperator(propertyIsLessThan);
		
		return filter;
	}	

	
