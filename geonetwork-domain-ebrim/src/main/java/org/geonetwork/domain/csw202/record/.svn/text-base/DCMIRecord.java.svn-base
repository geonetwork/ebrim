	/**
	 * For Jixb binding.
	 * 
	 * @return
	 */
	@SuppressWarnings("unused")
	private static Set<String> setFactory() {
		return new HashSet<String>();
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		DCMIRecord other = (DCMIRecord) obj;
		if (DCMI_Terms == null) {
			if (other.DCMI_Terms != null)
				return false;
		} else {
			if (DCMI_Terms.size() != other.DCMI_Terms.size()) {
				return false;
			}
			for (String term : DCMI_Terms) {
				if (!other.DCMI_Terms.contains(term))
					return false;
			}
		}
		return true;
	}

