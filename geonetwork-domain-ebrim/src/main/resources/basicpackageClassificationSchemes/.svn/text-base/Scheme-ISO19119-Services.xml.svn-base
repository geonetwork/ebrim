<?xml version="1.0" encoding="UTF-8"?>
<rim:ClassificationScheme 
  xmlns:rim="urn:oasis:names:tc:ebxml-regrep:xsd:rim:3.0"
  id="urn:x-ogc:specification:csw-ebrim:ClassificationScheme:ISO-19119:Services"
  objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationScheme"
  isInternal="true"
  nodeType="urn:oasis:names:tc:ebxml-regrep:NodeType:UniqueCode">
       <rim:Name>
          <rim:LocalizedString xml:lang="en" value="ISO 19119 Geographic services taxonomy"/>
       </rim:Name>
       <rim:Description>
          <rim:LocalizedString xml:lang="en" value="This taxonomy (defined in Subclause 8.3) is used to classify services according to their general computational characteristics."/>
       </rim:Description>
       
       <rim:ClassificationNode 
         objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
         parent="urn:x-ogc:specification:csw-ebrim:ClassificationScheme:ISO-19119:Services" 
         code="1000"
         id="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction">
          <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Human interaction services"/>
          </rim:Name>
          <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Services for providing user interfaces, graphics, multimedia, and for presentation of compound documents."/>
          </rim:Description>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1100" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Catalogue-Viewer">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Catalogue viewer"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a user to interact with a catalogue to locate, browse, and manage metadata about geographic data or geographic services."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1200" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Geo-Viewer">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Geographic viewer"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a user to view one or more feature collections or coverages. This viewer allows a user to interact with map data, e.g., displaying, overlaying and querying. An example is the viewer client generator defined in ISO 19128."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1300" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Spreadsheet-Viewer">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Geographic spreadsheet viewer"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a user to interact with multiple data objects and to request calculations similar to an arithmetic spreadsheet but extended to geographic data."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1400" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Service-Editor">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Service editor"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a user to control geographic processing services. Views include understanding a service, composing/scripting service chains, invoking a service, statusing a service, scheduling services, and invoking a service chain."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1500" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Workflow-Manager">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Workflow enactment manager"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides user interaction with a workflow enactment service."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1600" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Feature-Editor">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Geographic feature editor"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Geographic viewer that allows a user to interact with feature data, e.g.,displaying, querying. Supports feature annotation. The user controls view orientation, perspective, depth cueing, hidden-line/surface, light-sources, transparency, and texture etc."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1700" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Symbol-Editor">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Geographic symbol editor"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a human to select and manage symbol libraries. ISO 19117 is relevant to symbol libraries."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1800" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Generalization-editor">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Feature generalization editor"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a user to modify the cartographic characteristics of a feature or feature collection by simplifying its visualization, while maintaining its salient elements - the spatial equivalent of simplification."/>
         </rim:Description>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Human-Interaction" 
            code="1900" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Data-Viewer">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Geographic data-structure viewer"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Client service that allows a user to access part of dataset to see its internal structure. Allows user to request creation of new objects from parts of an object being browsed. Allow user to request a check of an object (e.g., type checking)."/>
         </rim:Description>
          </rim:ClassificationNode>
       </rim:ClassificationNode>
       
       <rim:ClassificationNode 
         objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
         parent="urn:x-ogc:specification:csw-ebrim:ClassificationScheme:ISO-19119:Services" 
         code="2000" 
         id="urn:x-ogc:specification:csw-ebrim:Service:Info-Management">
          <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Model/information management services"/>
          </rim:Name>
          <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Services for managing the development, manipulation, and storage of metadata, conceptual schemas, and datasets."/>
          </rim:Description>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
            code="2100" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Feature-Access">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Feature access service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to and management of a feature store. An access service may include a query that filters the data returned to the client. ISO 19125-1 is relevant to feature access."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
           code="2200" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Map-Access">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Map access service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to a geographic graphics (i.e., pictures of geographic data)."/>
         </rim:Description>
         </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
            code="2300" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Coverage-Access">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Coverage access service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to and management of a coverage store. An access service may include a query that filters the data returned to the client. ISO 19123 and ISO 19111 are relevant to coverage access."/>
         </rim:Description>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Coverage-Access" 
           code="2310" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Coverage-Sensors">
            <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Coverage Access Service - sensors"/>
            </rim:Name>
            <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Provides access to a coverage where the source of the coverage data is a real-time sensor, i.e., not a persistent store."/>
            </rim:Description>
         </rim:ClassificationNode>
        </rim:ClassificationNode>
        
        <rim:ClassificationNode 
          objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
          parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
          code="2400" 
          id="urn:x-ogc:specification:csw-ebrim:Service:Sensor-Description">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Sensor description service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides the description of a coverage sensor, including sensor location and orientation, as well as the sensor-s geometric, dynamic, and radiometric characteristics for geoprocessing purposes."/>
         </rim:Description>
         </rim:ClassificationNode>
         
        <rim:ClassificationNode 
          objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
          parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
          code="2500" 
          id="urn:x-ogc:specification:csw-ebrim:Service:Product-Access">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Product access service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to and management of a geographic product store. A product can be a predefined feature collection and metadata with known boundaries and content, corresponding to a paper map or report."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
           code="2600" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Feature-Type">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Feature type service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to and management of a store of feature type definitions. The static and dynamic information models for a feature type catalogue are provided in ISO 19110."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
           code="2700" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Catalogue">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Catalogue service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides discovery and management services on a store of metadata about instances. The metadata may be for dataset instances, e.g., dataset catalogue, or may contain service metadata, e.g., service catalogue."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
           code="2800" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Registry">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Registry service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to store of metadata about types or structured vocabularies that can be organized and related to each other. Example registries are information community registries, type dictionaries, service registries and schema registries."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
          objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
          parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
          code="2900" 
          id="urn:x-ogc:specification:csw-ebrim:Service:Gazetteer">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Gazetteer service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides access to a directory of instances of a class or classes of realworld phenomena containing some information regarding position. An information model for a gazetteer is provided by ISO 19112."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Info-Management" 
           code="2A00" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Order-Handling">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Order handling service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Provides the ability to order products from a provider including: formulation of quotes on orders, selection of geographic processing options, submission of an order, statusing of orders, and billing etc."/>
         </rim:Description>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Order-Handling" 
           code="2A10" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Standing-Order">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Standing order service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Order handling service that allows a user to request that a product over a geographic area be disseminated when it becomes available. Such dissemination includes receive, prepare (i.e., reformat, compress, decompress, etc.), prioritize, etc."/>
         </rim:Description>
         </rim:ClassificationNode>
       </rim:ClassificationNode>  
       </rim:ClassificationNode>
       
       <rim:ClassificationNode 
         objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
         parent="urn:x-ogc:specification:csw-ebrim:ClassificationScheme:ISO-19119:Services" 
         code="3000" 
         id="urn:x-ogc:specification:csw-ebrim:Service:Workflow">
          <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Workflow/task management services"/>
          </rim:Name>
          <rim:Description>
         <rim:LocalizedString xml:lang="en" value="Services for supporting specific tasks or work-related activities conducted by humans. These services support use of resources and development of products involving a sequence of activities or steps."/>
          </rim:Description>
          
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Workflow" 
           code="3100" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Chain-Definition">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Chain definition service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Enables the definition and execution of a chain by the workflow enactment service. This includes information about its starting and completion conditions, constituent activities and rules for navigating between them, etc."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Workflow" 
           code="3200" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Workflow-Enactment">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Workflow enactment service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Interprets a chain and controls the instantiation of services and sequencing of activities. This is done through one or more co-operating workflow management engines, which manages the execution."/>
         </rim:Description>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
           objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
           parent="urn:x-ogc:specification:csw-ebrim:Service:Workflow" 
           code="3300" 
           id="urn:x-ogc:specification:csw-ebrim:Service:Subscription">
         <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Subscription service"/>
         </rim:Name>
         <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Allow clients to register for notification about events of interest. Events are defined by a service that performs an activity resulting in the event. Events are catalogued by the subscription service."/>
         </rim:Description>
         </rim:ClassificationNode>
       </rim:ClassificationNode>
       
       <rim:ClassificationNode 
         objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
         parent="urn:x-ogc:specification:csw-ebrim:ClassificationScheme:ISO-19119:Services" 
         code="4000" 
         id="urn:x-ogc:specification:csw-ebrim:Service:Processing">
          <rim:Name>
         <rim:LocalizedString xml:lang="en" value="Processing services"/>
          </rim:Name>
          <rim:Description>
         <rim:LocalizedString xml:lang="en" value="Services that perform large-scale computations involving substantial amounts of data. Examples include services for providing the time of day, spelling checkers, and services that perform coordinate transformations."/>
          </rim:Description>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Processing" 
            code="4100" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Processing-Spatial">
           <rim:Name>
            <rim:LocalizedString xml:lang="en" value="Spatial processing service"/>
           </rim:Name>
           <rim:Description>
            <rim:LocalizedString xml:lang="en" value="Performs spatial operations on geographic features."/>
           </rim:Description>
           
           <rim:ClassificationNode 
             objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
             parent="urn:x-ogc:specification:csw-ebrim:Service:Processing-Spatial" 
             code="4110" 
             id="urn:x-ogc:specification:csw-ebrim:Service:Coordinate-Trans">
             <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Coordinate transformation service"/>
             </rim:Name>
             <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Transforms or converts coordinates between coordinate reference systems (see ISO 19111)."/>
             </rim:Description>
           </rim:ClassificationNode>
           
           <rim:ClassificationNode 
             objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
             parent="urn:x-ogc:specification:csw-ebrim:Service:Processing-Spatial" 
             code="4120" 
             id="urn:x-ogc:specification:csw-ebrim:Service:Orthorect">
             <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Orthorectification service"/>
             </rim:Name>
             <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Removes image displacement due to variation in terrain elevation. Orthorectification requires use of digital elevation data, usually in grid form."/>
             </rim:Description>
           </rim:ClassificationNode>
          </rim:ClassificationNode>
          
          <rim:ClassificationNode 
            objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
            parent="urn:x-ogc:specification:csw-ebrim:Service:Processing" 
            code="4200" 
            id="urn:x-ogc:specification:csw-ebrim:Service:Processing-Thematic">
           <rim:Name>
             <rim:LocalizedString xml:lang="en" value="Thematic processing service"/>
           </rim:Name>
           <rim:Description>
             <rim:LocalizedString xml:lang="en" value="Performs thematic operations on geographic features."/>
           </rim:Description>
           
           <rim:ClassificationNode 
             objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
             parent="urn:x-ogc:specification:csw-ebrim:Service:Processing-Thematic" 
             code="4210" 
             id="urn:x-ogc:specification:csw-ebrim:Service:Thematic-Classification">
             <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Thematic classification service"/>
             </rim:Name>
             <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Classifies regions of geographic data based on thematic attributes."/>
             </rim:Description>
           </rim:ClassificationNode>
           
            <rim:ClassificationNode 
             objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
             parent="urn:x-ogc:specification:csw-ebrim:Service:Processing-Thematic" 
             code="4220" 
             id="urn:x-ogc:specification:csw-ebrim:Service:Feature-Generalization">
             <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Feature generalization service"/>
             </rim:Name>
             <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Generalizes feature types in a feature collection to increase the effectiveness of communication by counteracting the undesirable effects of data reduction."/>
             </rim:Description>
           </rim:ClassificationNode>
           
           <rim:ClassificationNode 
             objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
             parent="urn:x-ogc:specification:csw-ebrim:Service:Processing-Thematic" 
             code="4230" 
             id="urn:x-ogc:specification:csw-ebrim:Service:Geoparsing">
             <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Geoparsing service"/>
             </rim:Name>
             <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Scans text resources for location-based references, such as a place names, addresses, postal codes, etc., in preparation for passage to a geocoding service."/>
             </rim:Description>
           </rim:ClassificationNode>
           
           <rim:ClassificationNode 
             objectType="urn:oasis:names:tc:ebxml-regrep:ObjectType:RegistryObject:ClassificationNode" 
             parent="urn:x-ogc:specification:csw-ebrim:Service:Processing-Thematic" 
             code="4240" 
             id="urn:x-ogc:specification:csw-ebrim:Service:Geocoding">
             <rim:Name>
               <rim:LocalizedString xml:lang="en" value="Geocoding service"/>
             </rim:Name>
             <rim:Description>
               <rim:LocalizedString xml:lang="en" value="Augments location-based text references with geographic coordinates (or some other spatial reference)."/>
             </rim:Description>
           </rim:ClassificationNode>
         </rim:ClassificationNode>
         
         <rim:ClassificationNode 
